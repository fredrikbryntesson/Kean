        public void Casts()
        {
            // integer - <#= @namespace1 #>
            Target.Integer.Point integer = new Target.Integer.Point(10, 20, 30);
            Target.<#= @namespace1 #>.Point <#= @namespace1 #> = integer;
            Verify(<#= @namespace1 #>.X, Is.EqualTo(10));
